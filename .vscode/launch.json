{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Launch Fingerprinter",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/fingerprinter",
            "args": ["8.8.8.8"], 
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}